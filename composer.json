{
    "name": "bytecraftnz/supabase-php",
    "description": "SupabasePHP is a library written in PHP to allow you to easily interact with Supabase ( https://supabase.io ) via their API.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Bytecraftnz\\SupabasePhp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Michael Osborn",
            "email": "osborn.michael@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^12.9",
        "illuminate/auth": "^12.9",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/psr7": "^2.7",
        "ext-json": "*"
    },
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}